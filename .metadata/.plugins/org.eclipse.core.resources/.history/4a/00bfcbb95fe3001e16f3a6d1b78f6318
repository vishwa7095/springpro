package com.Sathya.SpringEx1;

public class Patient {
		private String name;
		private int age;
		private Doctor doc;
		private Pharmacy phmcy;
		public Patient(String name, int age, Doctor doctor, Pharmacy pharmacy) {
			super();
			this.name = name;
			this.age = age;
			this.doc = doctor;
			this.phmcy = pharmacy;
		}
		public void Details()
		{
			doc.doctor();
			phmcy.medicine();
		}
}
