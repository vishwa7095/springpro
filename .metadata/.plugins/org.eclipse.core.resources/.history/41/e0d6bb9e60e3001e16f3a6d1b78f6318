package com.Sathya.SpringEx1;

public class Patient {
		private String name;
		private int age;
		public Patient(String name, int age) {
			super();
			this.name = name;
			this.age = age;
		}
		public void patientInfo()
		{
			System.out.println("patient:"+name);
			System.out.println("patient age:"+age);
		}
		
		 
 }
